{
    "name": "bench/monolith",
    "description": "Large monolith with many popular packages",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "laravel/framework": "^11.0",
        "doctrine/orm": "^3.0",
        "symfony/console": "^7.0",
        "symfony/http-foundation": "^7.0",
        "monolog/monolog": "^3.0",
        "league/flysystem": "^3.0",
        "phpunit/phpunit": "^11.0",
        "nesbot/carbon": "^3.0",
        "ramsey/uuid": "^4.7",
        "guzzlehttp/guzzle": "^7.8",
        "predis/predis": "^2.0",
        "league/csv": "^9.0",
        "spatie/laravel-permission": "^6.0",
        "intervention/image": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Domain\\": "src/Domain/",
            "Infrastructure\\": "src/Infrastructure/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/migrations"
        ]
    }
}
