{
    "name": "chrisnetonline/turbo-composer",
    "description": "Rust-powered Composer acceleration: parallel extraction + fast classmap generation",
    "type": "composer-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "composer-plugin-api": "^2.6"
    },
    "require-dev": {
        "composer/composer": "^2.6",
        "phpunit/phpunit": "^12.0",
        "carthage-software/mago": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "TurboComposer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TurboComposer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "class": "TurboComposer\\TurboInstallerPlugin"
    },
    "config": {
        "allow-plugins": {
            "carthage-software/mago": true
        }
    }
}
